   "source": [
    "def copy(file_name):\n",
    "    with open(file_name) as f:\n",
    "        f2 = open(file_name[:-4]+\"_copied.txt\",\"w\")\n",
    "        lines = []\n",
    "        for line in f:\n",
    "            lines.append(line)\n",
    "        f2.writelines(lines)\n",
    "        f2.close()\n",
    "        \n",
    "copy(\"sun.txt\")        "
   ]
